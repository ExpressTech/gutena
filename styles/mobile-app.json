                },
                {
                    "slug": "tertiary",
                    "color": "#5E6989",
                    "name": "Tertiary"
                }
            ],
            "duotone": [
				{
                    "colors": [
                        "#F24570",
                        "#353964"
                    ],
                    "slug": "foreground-and-primary",
                    "name": "Foreground and primary"
                }
			]
        },
        
        "custom": {
            "typography": {
                "font-family": {
                    "primary": "var(--wp--preset--font-family--manrope-font)",
                    "secondary": "var(--wp--preset--font-family--inter-font)"
                }
            }
        }
    },
    "styles": {
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "clamp(2.5rem, 2.5rem + ((1vw - 0.48rem) * 2.163), 3.625rem)",
