                }
            },
            "h3": {
                "typography": {
                    "fontSize": "clamp(1.125rem, 1.125rem + ((1vw - 0.48rem) * 0.24), 1.25rem)",
                    "lineHeight": "1.4"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "clamp(1rem, 1rem + ((1vw - 0.48rem) * 0.24), 1.125rem)",
                    "lineHeight": "1.46"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "clamp(0.875rem, 0.875rem + ((1vw - 0.48rem) * 0.24), 1rem)",
                    "lineHeight": "1.45"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "clamp(0.75rem, 0.75rem + ((1vw - 0.48rem) * 0.24), 0.875rem)",
